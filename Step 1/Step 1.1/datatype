int dataTypes(string ans) {
	// Write your code here
	if(ans == "Integer"){
		return 4;
	}
	else if(ans == "Long"){
		return 8;
	}
	else if(ans == "Float"){
		return 4;
	}
	else if(ans == "Double"){
		return 8;
	}
	else if(ans == "Character"){
		return 1;
	}
	else{
		return -1;
	}
}

