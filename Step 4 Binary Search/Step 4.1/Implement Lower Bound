#include<bits/stdc++.h>
int lowerBound(vector<int> arr, int n, int x) {
	// Write your code here

	//lower_bound it returns if element is present in 
	// the array then return that index if it is not so 
	// return the next immediate index
	int ind = lower_bound(arr.begin(), arr.end(), x) - arr.begin();
	return ind;
}