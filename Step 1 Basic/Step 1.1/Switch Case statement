double areaSwitchCase(int ch, vector<double> a) {
	// Write your code here
	switch(ch){
		case 1: 
		//circle area
		return (M_PI * a[0] * a[0]);
		break;

		case 2:
		//rectangle area
		return (a[0] * a[1]); 
		break;

		default:
		return -1.0;
	}
}
